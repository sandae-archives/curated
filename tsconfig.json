{
  "compilerOptions": {
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es5",
    "downlevelIteration": true,
    "preserveConstEnums": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "sourceMap": true,
    "outDir": ".build",
    "moduleResolution": "node",
    "lib": ["es2017"],
    "rootDir": "./src"
  }
}
